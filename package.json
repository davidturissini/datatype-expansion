{
  "name": "datatype-expansion",
  "version": "0.2.0",
  "description": "Utility tool to expand a given RAML type and create a canonical form",
  "main": "src/index.js",
  "module": "dist/datatype-expansion.es.js",
  "jsnext:main": "dist/datatype-expansion.es.js",
  "cjs:main": "dist/datatype-expansion.cjs.js",
  "umd:main": "dist/datatype-expansion.umd.js",
  "scripts": {
    "lint": "standard src/",
    "test-cov": "istanbul --include-all-sources cover -x index.es6.js --root src node_modules/mocha/bin/_mocha --report lcovonly -- -R dot --recursive --bail test/",
    "test-spec": "_mocha -R spec --recursive --bail test/",
    "test": "npm run lint && npm run test-cov",
    "build": "npm-run-all clean -p rollup:*",
    "clean": "rimraf dist && mkdirp dist",
    "rollup:cjs": "cross-env FORMAT=cjs rollup -c rollup.config.js -f cjs -n $npm_package_name -o $npm_package_cjs_main",
    "rollup:umd": "cross-env FORMAT=umd rollup -c rollup.config.js -f umd -n $npm_package_name -o $npm_package_umd_main",
    "rollup:esm": "cross-env FORMAT=es  rollup -c rollup.config.js -f es  -n $npm_package_name -o $npm_package_module"
  },
  "keywords": [
    "raml",
    "expansion",
    "json",
    "types"
  ],
  "author": "Mulesoft",
  "license": "Apache-2.0",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "browserify": "^14.4.0",
    "chai": "^4.0.2",
    "cross-env": "^5.0.5",
    "istanbul": "^0.4.5",
    "mkdirp": "^0.5.1",
    "mocha": "^3.4.2",
    "npm-run-all": "^4.0.2",
    "rimraf": "^2.6.1",
    "rollup": "^0.47.6",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.1.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "standard": "^10.0.2"
  },
  "dependencies": {
    "lodash": "^4.17.4"
  },
  "files": [
    "src/**",
    "LICENSE",
    "test/**"
  ]
}
